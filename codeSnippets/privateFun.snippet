<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>pr</Title>
			<Shortcut>pr</Shortcut>
			<Description>Code snippet for private method</Description>
			<Author>Luc Somers</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
      <Declarations>
        <Literal>
          <ID>methodname</ID>
          <ToolTip>name of the method</ToolTip>
          <Default>MethodName</Default>
        </Literal>
      </Declarations>
			<Code Language="csharp"><![CDATA[private void $methodname$()
      {
        $end$
      }]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>